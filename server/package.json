{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
  "build": "tsc",
  "start": "npm run build && node dist/server.js",
  "dev": "nodemon",
  "seed": "node dist/seeds/index.js",
  "db:drop": "npx sequelize-cli db:drop --config ../sequelize-config.js",
  "db:create": "npx sequelize-cli db:create --config ../sequelize-config.js",
  "db:migrate": "npx sequelize-cli db:migrate --config ../sequelize-config.js",
  "db:seed": "npx sequelize-cli db:seed:all --config ../sequelize-config.js"
},
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,json,js",
    "exec": "npm run build && node dist/server.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.14.0",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
